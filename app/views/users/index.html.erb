<h1>Users</h1>

<table>
    <tbody>
        <% @users.each do |user| %>
            <tr>
                <td><%= user.name %></td>
                <td><%= link_to "Edit user", edit_user_path(user)%> </td>
                <td><%= link_to "Show user", user_path(user)%> </td>
                <td><%= link_to "Delete user", user_path(user), :method => :delete, data: {confirm: "Are you sure?"}%> </td>
            </tr>
        <% end %>
    </tbody>
</table>

<%= link_to "New user", new_user_path%>